<% for (let i = 0, len = carousel.length; i < len; i++) { %>
<% const { img, alt, href } = carousel[i] %>

<li class="car-item <%= !i ? 'active' : '' %>">

  <a href="<%= href %>" target="_blank" class="car-lk">
    <img class="car-img" data-src="<%= img %> " src="/imgs/lazy.gif" alt="<%= alt %>" title="<%= alt %>" />
  </a>
</li>

<% } %>
